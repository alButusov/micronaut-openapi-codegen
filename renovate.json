{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": ["kokuwaio/renovate-config"],
	"packageRules": [
		{
			"matchPackageNames": ["io.kokuwa.micronaut:micronaut-openapi-codegen-it"],
			"enabled": false
		},{
			"groupName": "org.openapitools",
			"matchPackageNames": ["org.openapitools:openapi-generator", "org.openapitools:openapi-generator-maven-plugin"]
		}
	]
}
